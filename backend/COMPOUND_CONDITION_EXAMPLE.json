{
  "name": "Compound Condition Example",
  "description": "How to handle compound conditions like payment_category == 'Card Payment' && channel_type == 'Online Channel'",
  "row_generation_rules": [
    {
      "id": "rule_1", 
      "name": "Transaction Analysis with Compound Logic",
      "enabled": true,
      "output_columns": [
        {
          "id": "col_1",
          "name": "is_card_payment",
          "mapping_type": "dynamic",
          "dynamic_conditions": [
            {
              "condition_column": "payment_category",
              "operator": "==", 
              "condition_value": "Card Payment",
              "output_value": "1"
            }
          ],
          "default_value": "0"
        },
        {
          "id": "col_2",
          "name": "is_online_channel", 
          "mapping_type": "dynamic",
          "dynamic_conditions": [
            {
              "condition_column": "channel_type",
              "operator": "==",
              "condition_value": "Online Channel", 
              "output_value": "1"
            }
          ],
          "default_value": "0"
        },
        {
          "id": "col_3",
          "name": "compound_check",
          "mapping_type": "static",
          "static_value": "{is_card_payment} * {is_online_channel}"
        },
        {
          "id": "col_4", 
          "name": "transaction_sentiment",
          "mapping_type": "dynamic",
          "dynamic_conditions": [
            {
              "condition_column": "compound_check",
              "operator": "==",
              "condition_value": "1",
              "output_value": "Positive"
            }
          ],
          "default_value": "Neutral"
        }
      ],
      "priority": 0
    }
  ]
}